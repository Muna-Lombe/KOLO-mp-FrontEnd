/**app.wxss**/
@import "mp-stylesheets-master/fonts.wxss";
@import "mp-stylesheets-master/components/navbar.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

body {
  background-color: #FDFDFD;
}

.logo {
  background-size: fit;
  margin-top: 70rpx;
  width: 150rpx;
  height: 73rpx;
}

.navbar {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 0px 0px 20px 20px;
  height: 200rpx;
  width: 100%;
  background-image: linear-gradient(-60deg, #A9E4F4, #61afc7);
  position:sticky;
  top:0;
  z-index: 9;
}


