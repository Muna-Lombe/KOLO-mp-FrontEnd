/**app.wxss**/
@import "mp-stylesheets-master/fonts.wxss";
@import "mp-stylesheets-master/components/navbar.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.logo {
  background-size: fit;
  margin-top: 20px;
  width: 70px;
  height: 34px;
}

.navbar {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 0px 0px 20px 20px;
  height: 70px;
  width: 100%;
  background-color: #232323;
  position:sticky;
  top:0;
  z-index: 9;
}
